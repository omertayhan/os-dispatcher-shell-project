package dispatcher;

import java.io.IOException;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		//Txt üzerinden oknuna proseslerin ilk etapta atýlacaðý genel kuyruk tanýmlanýyor.
		Queue Processler = new Queue();
		try {
			GeneralFunctions Run = new GeneralFunctions();
			Processler=Run.ReadFile(args[0].toString()); //Prosesler okunup genel kuyruða atýlýyor.
		} catch (IOException e) {
			e.printStackTrace();
		}
		Interlayer.Decomposition(Processler); //Prosesler öncelik durumuna göre kuyruklara ayrýþtýrýlýp dispatcher baþlatýlýyor.

	}
			
}
