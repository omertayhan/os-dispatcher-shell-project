package dispatcher;

import java.io.IOException;

public class Program {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			args[0].toString();
		}
		catch(Exception e)
		{
			System.out.println("--->>Lütfen programý komut satýrý parametresi vererek çalýþtýrýnýz.\"---<<");
		}
		try {
			GeneralFunctions Run = new GeneralFunctions();
			Run.ReadFile(args[0].toString());
		} catch (IOException e) {
			e.printStackTrace();
		}
		
	}
			
}
